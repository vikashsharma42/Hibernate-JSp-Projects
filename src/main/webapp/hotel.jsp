<div class="hotel_form_container">
	<form action="#" method="post">
	<div class="hotel_name">
		<label class="hotel_label"><i class="fa fa-hotel"></i>Enter City</label><br>
		<input class="hotel_input" autocomplete="off" type="text" name="hotel" />
	</div>
	<div class="check_in">
		<label class="hotel_label"><i class="fa fa-calendar"></i>Enter City</label><br>
		<input class="hotel_input" autocomplete="off" type="date" name="checkin" aria-haspoup="true" min="2024-01-18T16:00:00.00Z" />
	</div>
	<div class="check_out">
		<label class="hotel_label"><i class="fa fa-calendar"></i>Enter City</label><br>
		<input class="hotel_input" autocomplete="off" type="date" name="checkin" aria-haspoup="true" min="2024-01-18T16:00:00.00Z" />
	</div>
	<div class="hotel_room">
		<label class="hotel_label"><i class="fa fa-users"></i>Rooms and Users</label><br>
		<input class="hotel_input" autocomplete="off" type="text" name="checkin" aria-haspoup="true" min="2024-01-18T16:00:00.00Z" />
	</div>
	<div class="hotel_button">
		<input type="button" value="Search Hotels">
	</div>
	</form>
</div>


